{
  "cesp_version": "1.0",
  "name": "friends",
  "display_name": "Friends",
  "version": "1.0.0",
  "author": {
    "name": "taejs",
    "github": "taejs"
  },
  "license": "CC-BY-NC-4.0",
  "language": "en",
  "categories": {
    "session.start": {
      "sounds": [
        {
          "file": "sounds/joey__how_you_doin.mp3",
          "label": "How you doin?"
        }
      ]
    },
    "task.acknowledge": {
      "sounds": [
        {
          "file": "sounds/ross__we_were_on_a_break.mp3",
          "label": "We were on a break!"
        }
      ]
    },
    "task.complete": {
      "sounds": [
        {
          "file": "sounds/ross__pivot.mp3",
          "label": "Pivot! Pivot! Pivot!"
        }
      ]
    },
    "task.error": {
      "sounds": [
        {
          "file": "sounds/janice__oh_my_god.mp3",
          "label": "Oh. My. God."
        }
      ]
    },
    "input.required": {
      "sounds": [
        {
          "file": "sounds/phoebe__my_eyes.mp3",
          "label": "My eyes! My eyes!"
        }
      ]
    },
    "resource.limit": {
      "sounds": [
        {
          "file": "sounds/ross__unagi.mp3",
          "label": "Unagi."
        }
      ]
    },
    "user.spam": {
      "sounds": [
        {
          "file": "sounds/phoebe__smelly_cat.mp3",
          "label": "Smelly cat, smelly cat."
        }
      ]
    }
  }
}
